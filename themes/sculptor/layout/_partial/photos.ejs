<section class="gallery">
  <% photos.forEach(function(photo) { %>
    <figure class="gallery__item">
      <img class="gallery__item-img" src="<%= photo.file %>" alt="<%= photo.name %>"/>
      <figcaption class="gallery__item-caption">
        <%= photo.caption %>
      </figcaption>
    </figure>
  <% }) %>
</section>
